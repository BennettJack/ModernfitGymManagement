// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
ID Int @id @default(autoincrement()) @unique
FirstName String
LastName String
Username String
Password String
Pin Int
Email String
Location String
}